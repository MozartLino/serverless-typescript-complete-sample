    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "standard-version": "^9.1.0"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "boom": "^7.3.0",
    "dotenv": "^8.2.0",
    "geolib": "^3.3.1",
    "mongodb": "3.5.11",
    "uuid": "^8.3.2"
  },
  "standard-version": {
    "skip": {
      "commit": true
    }
