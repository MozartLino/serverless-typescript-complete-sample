    "delivery"
  ],
  "author": "Ricardo Mozart Lino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardo-lino/test-ze-delivery/issues"
  },
  "homepage": "https://github.com/ricardo-lino/test-ze-delivery#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/parser": "^4.13.0",
    "@types/boom": "^7.3.0",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "mongodb-memory-server": "^6.9.2",
